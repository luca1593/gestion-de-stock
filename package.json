    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "angular-font-awesome": "^3.1.2",
    "bootstrap": "^5.2.2",
    "chart.js": "^4.4.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.7.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.28",
    "ngx-pagination": "^6.0.3",
    "rxjs": "~7.5.0",
    "save": "^2.9.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
